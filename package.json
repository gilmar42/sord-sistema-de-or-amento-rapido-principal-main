{
  "name": "sored---sistema-de-orçamento-rápido",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "dev:bg": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Start-Process -FilePath 'node' -ArgumentList 'node_modules\\vite\\bin\\vite.js --host 0.0.0.0 --port 5173' -WindowStyle Hidden\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd sord-frontend && npm test",
    "test:backend": "cd sord-backend && npm test",
    "test:all": "npm run test:frontend && npm run test:backend",
    "test:watch": "npm run test:backend -- --watch",
    "test:coverage": "npm run test:frontend:coverage && npm run test:backend:coverage",
    "test:frontend:coverage": "cd sord-frontend && npm run test:coverage",
    "test:backend:coverage": "cd sord-backend && npm run test:coverage",
    "test:ci": "npm run test:frontend:ci && npm run test:backend:ci",
    "test:frontend:ci": "cd sord-frontend && npm run test:ci",
    "test:backend:ci": "cd sord-backend && npm run test:ci",
    "test:production": "jest --config jest.config.cjs --testPathPatterns=\"production|e2e\"",
    "validate:production": "node scripts/validate-production.js",
    "debug:production": "node debug-production.js",
    "check:types": "tsc --noEmit",
    "check:backend": "cd sord-backend && tsc --noEmit",
    "check:all": "npm run check:types && npm run check:backend"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@types/uuid": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^5.2.0"
  }
}
